                <groupId>com.lovecyy</groupId>
                <artifactId>relaxed-common-idempotent</artifactId>
                <version>${reversion}</version>
            </dependency>
            <!--多数据源-->
            <dependency>
                <groupId>com.lovecyy</groupId>
                <artifactId>relaxed-common-datasource</artifactId>
                <version>${reversion}</version>
            </dependency>
            <!--数据域-->
            <dependency>
                <groupId>com.lovecyy</groupId>
                <artifactId>relaxed-common-datascope</artifactId>
                <version>${reversion}</version>
            </dependency>
            <!--多租户 独立数据库 多schema-->
            <dependency>
                <groupId>com.lovecyy</groupId>
                <artifactId>relaxed-common-tenant</artifactId>
                <version>${reversion}</version>
            </dependency>
            <!--Spring Security jwt-->
            <dependency>
                <groupId>com.lovecyy</groupId>
                <artifactId>relaxed-common-security</artifactId>
                <version>${reversion}</version>
            </dependency>
            <!--cache-->
            <dependency>
                <groupId>com.lovecyy</groupId>
                <artifactId>relaxed-common-cache</artifactId>
                <version>${reversion}</version>
            </dependency>
            <!--risk engine-->
            <dependency>
                <groupId>com.lovecyy</groupId>
                <artifactId>relaxed-common-risk-engine</artifactId>
                <version>${reversion}</version>
            </dependency>
            <!--config-->
            <dependency>
                <groupId>com.lovecyy</groupId>
                <artifactId>relaxed-common-config</artifactId>
                <version>${reversion}</version>
            </dependency>
            <!--通用工具类-->
            <dependency>
                <groupId>com.lovecyy</groupId>
                <artifactId>relaxed-common-util</artifactId>
                <version>${reversion}</version>
            </dependency>
            <!--扩展mybatis plus-->
            <dependency>
                <groupId>com.lovecyy</groupId>
                <artifactId>relaxed-extend-mybatis-plus</artifactId>
                <version>${reversion}</version>
            </dependency>

            <!--sftp客户端-->
            <dependency>
                <groupId>com.lovecyy</groupId>
                <artifactId>relaxed-spring-boot-starter-sftp</artifactId>
                <version>${reversion}</version>
            </dependency>
            <!--ding talk-->
            <dependency>
                <groupId>com.lovecyy</groupId>
                <artifactId>relaxed-spring-boot-starter-dingtalk</artifactId>
                <version>${reversion}</version>
            </dependency>
            <!--分布式调度任务xxl-job-->
            <dependency>
                <groupId>com.lovecyy</groupId>
                <artifactId>relaxed-spring-boot-starter-job</artifactId>
                <version>${reversion}</version>
            </dependency>
            <!--邮件发送-->
            <dependency>
                <groupId>com.lovecyy</groupId>
                <artifactId>relaxed-spring-boot-starter-mail</artifactId>
                <version>${reversion}</version>
            </dependency>
            <!--日志记录-->
            <dependency>
                <groupId>com.lovecyy</groupId>
                <artifactId>relaxed-spring-boot-starter-log</artifactId>
                <version>${reversion}</version>
            </dependency>
            <!--xss 过滤-->
            <dependency>
                <groupId>com.lovecyy</groupId>
                <artifactId>relaxed-spring-boot-starter-xss</artifactId>
                <version>${reversion}</version>
            </dependency>
            <!--swagger 接口文档 -->
            <dependency>
                <groupId>io.swagger</groupId>
                <artifactId>swagger-models</artifactId>
                <version>${swagger-models.version}</version>
            </dependency>
            <dependency>
                <groupId>io.swagger</groupId>
                <artifactId>swagger-annotations</artifactId>
                <version>${swagger-models.version}</version>
            </dependency>
            <dependency>
                <groupId>com.lovecyy</groupId>
                <artifactId>relaxed-spring-boot-starter-swagger</artifactId>
                <version>${reversion}</version>
            </dependency>
            <!--缓存模块-->
            <dependency>
                <groupId>com.lovecyy</groupId>
                <artifactId>relaxed-spring-boot-starter-cache</artifactId>
                <version>${reversion}</version>
            </dependency>
            <!--EXCEL-->
            <dependency>
                <groupId>com.lovecyy</groupId>
                <artifactId>relaxed-spring-boot-starter-easyexcel</artifactId>
                <version>${reversion}</version>
            </dependency>
            <!--mybatis plus extension,包含了mybatis plus core-->
            <dependency>
                <groupId>com.baomidou</groupId>
                <artifactId>mybatis-plus-extension</artifactId>
                <version>${mybatis-plus.version}</version>
            </dependency>
            <dependency>
                <groupId>com.baomidou</groupId>
                <artifactId>mybatis-plus-core</artifactId>
                <version>${mybatis-plus.version}</version>
            </dependency>
            <dependency>
                <groupId>com.baomidou</groupId>
                <artifactId>mybatis-plus-annotation</artifactId>
                <version>${mybatis-plus.version}</version>
            </dependency>
            <dependency>
                <groupId>com.baomidou</groupId>
                <artifactId>mybatis-plus-boot-starter</artifactId>
                <version>${mybatis-plus.version}</version>
            </dependency>
            <dependency>
                <groupId>com.baomidou</groupId>
                <artifactId>dynamic-datasource-spring-boot-starter</artifactId>
                <version>${dynamic-datasource.version}</version>
            </dependency>
            <!--jasypt加密模块-->
            <dependency>
                <groupId>com.github.ulisesbocchio</groupId>
                <artifactId>jasypt-spring-boot-starter</artifactId>
                <version>${jasypt.version}</version>
            </dependency>
            <!--mybatis-->
            <dependency>
                <groupId>org.mybatis</groupId>
                <artifactId>mybatis</artifactId>
                <version>${mybatis.version}</version>
            </dependency>
            <!--Sql 解释器-->
            <dependency>
                <groupId>com.github.jsqlparser</groupId>
                <artifactId>jsqlparser</artifactId>
                <version>${jsqlparse.version}</version>
            </dependency>
            <!--IP 地址转换-->
